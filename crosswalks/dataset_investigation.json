
{
  "idfield": "oid",
  "source_type": "dataset",
  "workflow_step": "investigation",
  "dest_type": "dataRecord",

  "workflow": {
    "stageold1": "stagenew1",
    "stageold2": "stagenew2"
  },

  "permissions": {
    "user_id": "email",
    "permissions": {
      "_owner_": [ "view", "edit" ],
      "contributor_ci": [ "view", "edit" ],
      "contributor_data_manager": [ "view", "edit" ],
      "contributors": [ "view" ],
      "contributor_supervisor": [ "view" ]
    }
  },

  "required": [
    "title",
    "description"
  ],

  "ignore": [
    "action",
    "oid",
    "objMeta"
  ],
  "fields": {

    "dc_title":"title",

    "description": "description",

    "finalKeywords": {
      "name": "_",
      "type": "record",
      "repeatable": 1
    },

    "bibo:DataLocation": {
      "name": "dataLocations",
      "type": "record",
      "handler": "DataLocations",
      "repeatable": 1,
      "fields": {
        "dc:location": "location",
        "dc:type": "type",
        "dc:privacy": "privacy",
        "dc:notes": "notes"
      }
    },

    "foaf:fundedBy.foaf:Agent": {
      "name": "_",
      "type": "record",
      "repeatable": 1,
      "handler": "FundingBody",
      "fields": {
        "dc:identifier": "dc_identifier",
        "skos:prefLabel": "dc_title"
      }
    },

    "foaf:fundedBy.vivo:Grant": {
      "name": "_",
      "type": "record",
      "repeatable": 1,
      "handler": "GrantBody",
      "fields": {
        "warning": "mint_lookup",
        "dc:identifier": "dc:identifier",
        "skos.prefLabel": "dc:title",
        "redbox:grantNumber": "grant_number"
      }
    },

    "dc:subject.anzsrc:for": {
      "name": "_",
      "type": "record",
      "handler": "ForSeo",
      "repeatable": 1,
      "fields": {
        "skos:prefLabel": "skos:prefLabel",
        "rdf:resource": "rdf:resource"
      }
    },

    "locrel:rth.foaf:Person": {
      "name": "contributor_ci",
      "type": "record",
      "handler": "Person",
      "role": "Chief Investigator",
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "givenname",
        "foaf:familyName": "familyname"
      }
    },

    "locrel:prc.foaf:Person": {
      "name": "contributor_data_manager",
      "type": "record",
      "handler": "Person",
      "role": "Data Manager",
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "givenname",
        "foaf:familyName": "familyname"
      }
    },

    "locrel:clb.foaf:Person": {
      "name": "contributors",
      "type": "record",
      "handler": "Person",
      "role": "Contributor",
      "repeatable": 1,
      "fields": {
        "person.dc:identifier": "dc:identifier",
        "person.foaf:title": "honorific",
        "person.foaf:email": "email",
        "person.foaf:givenName": "givenname",
        "person.foaf:familyName": "familyname"
      }
    },

    "swrc:supervisor.foaf:Person": {
      "name": "contributor_supervisor",
      "type": "record",
      "handler": "Person",
      "role": "Supervisor",
      "fields": {
        "dc:identifier": "dc:identifier",
        "foaf:title": "honorific",
        "foaf:email": "email",
        "foaf:givenName": "givenname",
        "foaf:familyName": "familyname"
      }
    },

    "dc:accessRights": {
      "name": "_",
      "type": "valuemap",
      "map" : {
        "permission from the data manager": "manager",
        "open, free access under license": "open",
        "no access due to sensitivities": "none"
      }
    }
  }
}
