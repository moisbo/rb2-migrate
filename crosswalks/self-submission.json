
{
  "idfield": "oid",
  "source_type": "self-submission",
  "workflow_step": "self-submission-draft",
  "dest_type": "dataRecord",

  "workflow": {
    "stageold1": "stagenew1",
    "stageold2": "stagenew2"
  },

  "permissions": {
    "user_id": "email",
    "permissions": {
      "_owner_": [ "view", "edit" ],
      "contributor_ci": [ "view", "edit" ],
      "contributor_data_manager": [ "view", "edit" ],
      "contributors": [ "view" ],
      "contributor_supervisor": [ "view" ]
    }
  },

  "required": [
    "title",
    "description"
  ],

  "ignore": [
    "action",
    "oid",
    "objMeta"
  ],
  "fields": {
    "dc:title": "title",

    "dc:description":"description",

    "dc_description":"description",

    "description": "description",

    "bibo:DataLocation": {
      "name": "dataLocations",
      "handler": "DataLocations",
      "repeatable": 1,
      "fields": {
        "dc:location": "location",
        "dc:type": "type",
        "dc:privacy": "privacy",
        "dc:notes": "notes"
      }
    },

    "keywords": {
      "name": "finalKeywords",
      "type": "record",
      "repeatable": 1
    },


    "redbox:retentionPeriod.dc:date": {
      "name": "_",
      "type": "valuemap",
      "map": {
        "1year": "1year",
        "5years": "5years",
        "7years": "7years",
        "25years": "25years",
        "permanent": "permanent"
      }
    },

    "redbox:retentionPeriod.dc:date.skos:note": {
      "name": "_",
      "type": "valuemap",
      "map": {
        "heritage": "heritage",
        "controversial": "controversial",
        "ofinterest": "ofinterest",
        "costly_impossible": "costly_impossible",
        "commercial": "commercial"
      }
    }

  }
}
